#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(){
	int mat[3][3] = {{5,4,2}, {7,9,8}};
	int mat1[3][3] = {{4,3,2}, {7,2,1}};
	int suma [3][3];
	
	for (int i = 0; i < 3; i ++){
		for (int j = 0; j < 3; j ++){
			suma [i][j] = mat[i][j] + mat1 [i][j];
		}
	}
	
	for (int i = 0; i < 3; i ++){
		for (int j = 0; j < 3; j ++){
			printf("%i", suma[i][j]);
		}
		printf("\n");
	}
	
	
}
