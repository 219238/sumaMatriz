#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(){
	srand(time(NULL));
	int filas,columnas;
	int valores=1+rand()%(10-1);
	printf("INGRESA EL NUMERO DE FILAS");
	scanf("%i",&filas);
	printf("INGRESA EL NUMERO DE COLUMNAS");
	scanf("%i",&columnas);
	int mat[filas][columnas], mat1[filas][columnas], mat2[filas][columnas];
	for (int i=0; i< filas;i++ ){
		for(int j=0; i< columnas;j++){
			mat[i][j]= valores;
			printf("[%i][%i]\n", mat[i][j]);
			printf("\n\t");
		}
	}
	for (int i=0; i< filas;i++ ){
	for(int j=0; i< columnas;j++){
			//int valores=1+rand()%(10-1);
			mat1[i][j]= valores;
			printf("[%i][%i]\n", mat1[i][j]);
			printf("\n\t");
		}
	}
	for (int i=0; i< filas;i++ ){
	for(int j=0; i< columnas;j++){
			mat2[i][j]=mat[i][j]+mat1[i][j];
			printf("[%i][%i]\n", mat2[i][j]);
			printf("\n\t");
		}
	}
	
}
