#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(){
	srand(time(NULL));

	int mat[3][3], mat1[3][3], mat2[3][3],suma;
	for(int i = 0; i < 3; i ++){
		for (int j = 0; j < 3; j ++){
			int aleatorios = 1+rand()%(30-1);
			mat[i][j] = aleatorios;
		}
	}
	for(int k = 0; k < 3; k ++){
		for (int l = 0; l < 3; l ++){
			int aleatorios = 1+rand()%(30-1);
			mat[k][l] = aleatorios;
		}
	}
	for(int m = 0; m < 3; m ++){
		for (int n = 0; n < 3; n ++){
			suma = mat[m][n] + mat2 [m][n];
			printf("[%i][%i][%i] \n", mat[m][n]);
		}
	}
}
